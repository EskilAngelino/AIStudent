#Task 1: Variables and Data Types
# Contact information
name: str = ("John Doe")
age: int = (25)
phone_number: str = ("123-456-7890")
email: str = ("john@example.com")

# Display contact information
print("Contact Information:")
print(f"Name: {name}")
print(f"Age: {age}")
print(f"Phone Number: {phone_number}")
print(f"Email: {email}")
